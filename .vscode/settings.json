{
	"editor.renderWhitespace": "all",
	"editor.minimap.enabled": false,
	"editor.formatOnSave": false,
	"javascript.format.enable": false,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true
	},
